#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-107-gab6ae79)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x140bfc0 .scope module, "forwardingTest" "forwardingTest" 2 3;
 .timescale -9 -12;
v0x14333e0_0 .var "MEM_data", 31 0;
v0x14334c0_0 .var "MEM_reg", 4 0;
v0x1433590_0 .var "WB_data", 31 0;
v0x1433690_0 .var "WB_reg", 4 0;
v0x1433760_0 .net "data", 31 0, v0x1432fd0_0;  1 drivers
v0x1433850_0 .var "targetData", 31 0;
v0x1433920_0 .var "targetReg", 4 0;
S_0x141d0a0 .scope module, "forwarding" "forwarding" 2 7, 3 2 0, S_0x140bfc0;
 .timescale -9 -12;
    .port_info 0 /INPUT 5 "targetReg"
    .port_info 1 /INPUT 32 "targetData"
    .port_info 2 /INPUT 5 "MEM_reg"
    .port_info 3 /INPUT 32 "MEM_data"
    .port_info 4 /INPUT 5 "WB_reg"
    .port_info 5 /INPUT 32 "WB_data"
    .port_info 6 /OUTPUT 32 "data"
v0x141d340_0 .net "MEM_data", 31 0, v0x14333e0_0;  1 drivers
v0x1432d20_0 .net "MEM_reg", 4 0, v0x14334c0_0;  1 drivers
v0x1432e00_0 .net "WB_data", 31 0, v0x1433590_0;  1 drivers
v0x1432ef0_0 .net "WB_reg", 4 0, v0x1433690_0;  1 drivers
v0x1432fd0_0 .var "data", 31 0;
v0x1433100_0 .net "targetData", 31 0, v0x1433850_0;  1 drivers
v0x14331e0_0 .net "targetReg", 4 0, v0x1433920_0;  1 drivers
E_0x141dee0 .event edge, v0x14331e0_0, v0x1433100_0;
    .scope S_0x141d0a0;
T_0 ;
    %wait E_0x141dee0;
    %load/vec4 v0x14331e0_0;
    %dup/vec4;
    %load/vec4 v0x1432d20_0;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %load/vec4 v0x1432ef0_0;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %load/vec4 v0x1433100_0;
    %store/vec4 v0x1432fd0_0, 0, 32;
    %jmp T_0.3;
T_0.0 ;
    %load/vec4 v0x141d340_0;
    %store/vec4 v0x1432fd0_0, 0, 32;
    %jmp T_0.3;
T_0.1 ;
    %load/vec4 v0x1432e00_0;
    %store/vec4 v0x1432fd0_0, 0, 32;
    %jmp T_0.3;
T_0.3 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x140bfc0;
T_1 ;
    %vpi_call 2 10 "$display", "Starting forwarding testing" {0 0 0};
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x14333e0_0, 0, 32;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x1433590_0, 0, 32;
    %pushi/vec4 16, 0, 5;
    %store/vec4 v0x14334c0_0, 0, 5;
    %pushi/vec4 1, 0, 5;
    %store/vec4 v0x1433690_0, 0, 5;
    %pushi/vec4 21, 0, 5;
    %store/vec4 v0x1433920_0, 0, 5;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x1433850_0, 0, 32;
    %delay 100000, 0;
    %load/vec4 v0x1433760_0;
    %cmpi/ne 3, 0, 32;
    %jmp/0xz  T_1.0, 6;
    %vpi_call 2 16 "$display", "Test 1 failed - %d", v0x1433760_0 {0 0 0};
T_1.0 ;
    %pushi/vec4 16, 0, 5;
    %store/vec4 v0x1433920_0, 0, 5;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x1433850_0, 0, 32;
    %delay 100000, 0;
    %load/vec4 v0x1433760_0;
    %cmpi/ne 1, 0, 32;
    %jmp/0xz  T_1.2, 6;
    %vpi_call 2 20 "$display", "Test 2 failed - %d", v0x1433760_0 {0 0 0};
T_1.2 ;
    %pushi/vec4 1, 0, 5;
    %store/vec4 v0x1433920_0, 0, 5;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x1433850_0, 0, 32;
    %delay 100000, 0;
    %load/vec4 v0x1433760_0;
    %cmpi/ne 2, 0, 32;
    %jmp/0xz  T_1.4, 6;
    %vpi_call 2 24 "$display", "Test 3 failed - %d", v0x1433760_0 {0 0 0};
T_1.4 ;
    %pushi/vec4 16, 0, 5;
    %store/vec4 v0x1433690_0, 0, 5;
    %pushi/vec4 16, 0, 5;
    %store/vec4 v0x1433920_0, 0, 5;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x1433850_0, 0, 32;
    %delay 100000, 0;
    %load/vec4 v0x1433760_0;
    %cmpi/ne 1, 0, 32;
    %jmp/0xz  T_1.6, 6;
    %vpi_call 2 29 "$display", "Test 1 failed - %d", v0x1433760_0 {0 0 0};
T_1.6 ;
    %vpi_call 2 31 "$display", "Finished forwarding testing" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
